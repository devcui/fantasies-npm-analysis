{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Npm-Analysis Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}/node_modules/.bin/vscode",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "preLaunchTask": "npm-analysis: build"
    }
  ]
}